{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --mode development",
    "build": "run-p build-only",
    "build-dev": "run-p build-only-dev",
    "build-stg": "run-p build-only-stg",
    "preview": "vite preview",
    "build-only": "vite build --mode production",
    "build-only-dev": "vite build --mode development",
    "build-only-stg": "vite build --mode staging",
    "type-check": "vue-tsc --noEmit",
    "format": "prettier \"./**/*.{vue,ts}\" -w --ignore-path .gitignore",
    "lint": "eslint \"./**/*.{vue,ts}\" --fix --ignore-path .gitignore",
    "fix": "npm run format & npm run lint"
  },
  "dependencies": {
    "@mdi/font": "^7.3.67",
    "axios": "^1.5.1",
    "dayjs": "^1.11.10",
    "file-saver": "^2.0.5",
    "lodash": "^4.17.21",
    "pinia": "^2.1.6",
    "vee-validate": "^4.11.8",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5",
    "vuetify": "^3.3.20",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.7",
    "@types/node": "^20.8.3",
    "@types/webfontloader": "^1.6.35",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.51.0",
    "eslint-plugin-vue": "^9.17.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "sass": "^1.69.0",
    "sass-loader": "^13.3.2",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-eslint2": "^4.1.0",
    "vite-plugin-vuetify": "^1.0.2",
    "vue-tsc": "^1.8.5"
  }
}
